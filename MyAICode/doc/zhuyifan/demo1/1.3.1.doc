                            </w:rPr>
                            <w:t>6 Cookie Security: HTTPOnly not Set</w:t>
                        </w:r>
                        <w:proofErr w:type="spellEnd"/>
                        <w:proofErr w:type="gramEnd"/>
                        <w:r w:rsidR="002C6ED7">
